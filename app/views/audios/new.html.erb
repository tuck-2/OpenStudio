<h1>楽曲投稿ページ</h1>
<%= form_with model: @audio, local:true do |f| %>
  <%= f.label :file %>
  <%= f.file_field :file, :size => 140 %>
  <%= f.label :img %>
  <%= f.file_field :img %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.submit "投稿" %>
<% end %>